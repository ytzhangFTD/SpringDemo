<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean class="org.springframework.beans.factory.annotation.AutowiredAnnotationBeanPostProcessor"/>
    <bean id="caseLoader" class="com.ytzhang.test.batchload.loader.CaseLoader"/>
    <bean id="caseLoadFacadeImpl" class="com.ytzhang.test.batchload.CaseLoadFacadeImpl"/>
    <bean id="caseReader" class="com.ytzhang.test.batchload.reader.CaseExcelReader"/>





    <!--批量导入职责链设计-->
    <bean id="validHandler" class="com.ytzhang.test.batchload2.ValidHandler">
        <property name="handler" ref="endHandler"/>
    </bean>
    <bean id="caseHandler" class="com.ytzhang.test.batchload2.CaseHandler">
        <property name="handler" ref="suitRequestHandler"/>
    </bean>
    <bean id="suitRequestHandler" class="com.ytzhang.test.batchload2.SuitRequestHandler">
        <property name="handler" ref="suitEntityHandler"/>
    </bean>
    <bean id="suitEntityHandler" class="com.ytzhang.test.batchload2.SuitEntityHandler">
        <property name="handler" ref="suitEvidenceHandler"/>
    </bean>

    <bean id="suitEvidenceHandler" class="com.ytzhang.test.batchload2.SuitEvidenceHandler">
        <property name="handler" ref="endHandler"/>
        <property name="suitEvidenceLoader" ref="suitEvidenceLoader"/>
    </bean>
    <bean id="endHandler" class="com.ytzhang.test.batchload2.EndHandler"/>


    <bean id="suitEvidenceLoader" class="com.ytzhang.test.batchload2.loader.SuitEvidenceLoader"/>
</beans>